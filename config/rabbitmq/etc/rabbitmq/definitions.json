{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    },
    {
      "name": "price-service",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ""
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "price-service",
      "vhost": "/",
      "configure": "^(pas|pad|pra).*",
      "write": "^(pas|pad|pra).*",
      "read": "^(pas|pad|pra).*"
    }
  ],
  "exchanges": [
    {
      "name": "pas.exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {
        "alternate-exchange": "dlx.exchange"
      }
    },
    {
      "name": "pad.exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {
        "alternate-exchange": "dlx.exchange"
      }
    },
    {
      "name": "dlx.exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "pas.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "dlx.exchange",
        "x-dead-letter-routing-key": "pas.dlq",
        "x-message-ttl": 86400000,
        "x-max-priority": 10,
        "x-queue-mode": "lazy",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "pad.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "dlx.exchange",
        "x-dead-letter-routing-key": "pad.dlq",
        "x-message-ttl": 86400000,
        "x-max-priority": 10,
        "x-queue-mode": "lazy",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "pas.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-queue-mode": "lazy",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "pad.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-queue-mode": "lazy",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    }
  ],
  "bindings": [
    {
      "source": "pas.exchange",
      "vhost": "/",
      "destination": "pas.queue",
      "destination_type": "queue",
      "routing_key": "pas.key",
      "arguments": {}
    },
    {
      "source": "pad.exchange",
      "vhost": "/",
      "destination": "pad.queue",
      "destination_type": "queue",
      "routing_key": "pad.key",
      "arguments": {}
    },
    {
      "source": "dlx.exchange",
      "vhost": "/",
      "destination": "pas.dlq",
      "destination_type": "queue",
      "routing_key": "pas.dlq",
      "arguments": {}
    },
    {
      "source": "dlx.exchange",
      "vhost": "/",
      "destination": "pad.dlq",
      "destination_type": "queue",
      "routing_key": "pad.dlq",
      "arguments": {}
    }
  ]
}